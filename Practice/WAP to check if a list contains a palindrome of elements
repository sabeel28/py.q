# WAP to check if a list contains a
# palindrome of elements

list1=[1,2,3,2,1]
list1_copy=list1.copy()
list1.reverse()
if(list1_copy==list1):
    print("palindrome")
else:
    print("not palindrome")
----------------------------------------
list1=['a','s','a','m']
list1_copy=list1.copy()
list1.reverse()
if(list1_copy==list1):
    print("palindrome")
else:
    print("not palindrome") 
