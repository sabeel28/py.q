def cal_prod(a=1,b=1): #default values means when there is no argumnet is passed
    print(a*b)
    return a*b
cal_prod() #1
--------
def cal_prod(a,b=1):
    print(a*b)
    return a*b
cal_prod(2) #output=2
-------
def cal_prod(a,b):
    print(a*b)
    return a*b
cal_prod(2,3) #6
